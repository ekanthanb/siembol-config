{
  "rule_name": "username",
  "rule_author": "siembol",
  "rule_version": 3,
  "source_type": "linux.syslogs",
  "matchers": [
    {
      "is_enabled": true,
      "matcher_type": "IS_IN_SET",
      "is_negated": false,
      "field": "original_string",
      "case_insensitive": true,
      "data": "test"
    }
  ],
  "table_mapping": {
    "table_name": "username",
    "joining_key": "${original_string}",
    "enriching_fields": [
      {
        "table_field_name": "username",
        "event_field_name": "enriched_username"
      }
    ]
  }
}